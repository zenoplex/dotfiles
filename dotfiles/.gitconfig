[user]
	name = Gen Ichino
	email = zenoplex@gmail.com
	username = zenoplex
[color]
	ui = auto
[alias]
	co = checkout
	s = status -s
	br = branch
	brdm = !git branch --merged | egrep -v '(^\\*|master|main)'
[ghq]
	root = ~/projects/
[init]
	defaultBranch = main
[diff]
	tool = p4merge
[merge]
	tool = p4merge
[rebase]
	autosquash = true
[mergetool "p4merge"]
	path = /Applications/p4merge.app/Contents/Resources/launchp4merge
